#include <unistd.h>
#include <stdio.h>

void	ft_print_numbers(void)
{
	char mat;

	mat = '0';
	while (mat <= '9')
	{
		write(1, &mat, 1);
		mat++;
	}
}

int main(void)
{
	ft_print_numbers();
}
