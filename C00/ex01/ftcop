#include<stdio.h>
#include<unistd.h>

void ft_print_alphabet()
{
	char alpha;
		alpha= 'a';
		while(alpha <= 'z')
 			{
				write(1, &alpha, 1);
				alpha++;
			}

}
int main(void)
{
	ft_print_alphabet();
}
